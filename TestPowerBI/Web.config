<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="ApplicationID" value="AppID" />
    <add key="ApplicationSecret" value="AppSecret" />
    <add key="TenantId" value="TanentID" />
    <add key="WorkspaceId" value="workstationid" />
    <add key="ReportId" value="reportid" />
    <add key="AADAuthorityUri" value="https://login.windows.net/common/" />
    <add key="PowerBiApiUrl" value="https://api.powerbi.com/" />
    <add key="pbiUsername" value="user"/>
    <add key="pbiPassword" value="password"/>
    <add key="scope" value="https://analysis.windows.net/powerbi/api/.default"/>
    <!--<add key="Scope" value="https://analysis.windows.net/powerbi/api/Report.Read.All,https://analysis.windows.net/powerbi/api/Dashboard.Read.All" />-->
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5.2" />
    <httpRuntime targetFramework="4.5.2" />
    <customErrors mode="Off" />
  </system.web>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" culture="neutral" publicKeyToken="30ad4fe6b2a6aeed" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.IdentityModel.Clients.ActiveDirectory" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.5.0.0" newVersion="4.5.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>